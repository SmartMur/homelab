# Popup Homelab Environment Variables
# Complete homelab stack configuration

# Domain Configuration
DOMAIN=smartmur.lab
EMAIL=info@smartmur.lab

# Cloudflare API Token
CF_API_TOKEN=your_cloudflare_dns_api_token

# Timezone
TZ=UTC

# Traefik Dashboard
TRAEFIK_DASHBOARD_AUTH=admin:$$apr1$$CHANGE_ME_GENERATE_HTPASSWD

# Pi-hole
PIHOLE_PASSWORD=CHANGE_ME_SECURE_PASSWORD

# Authelia
AUTHELIA_JWT_SECRET=CHANGE_ME_GENERATE_64_CHARS
AUTHELIA_SESSION_SECRET=CHANGE_ME_GENERATE_64_CHARS
AUTHELIA_STORAGE_ENCRYPTION_KEY=CHANGE_ME_GENERATE_64_CHARS

# Homepage Dashboard
HOMEPAGE_VAR_TITLE=My Homelab

# Watchtower
WATCHTOWER_HTTP_API_TOKEN=CHANGE_ME_GENERATE_32_CHARS

# CrowdSec
CROWDSEC_LAPI_KEY=CHANGE_ME_GENERATE_64_CHARS
CROWDSEC_BOUNCER_KEY=CHANGE_ME_GENERATE_64_CHARS

# Network
NETWORK_NAME=proxy
