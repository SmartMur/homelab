# Dynamic DNS Environment Variables
# Copy this file to .env and fill in your actual values

# Cloudflare Configuration
CLOUDFLARE_ZONE_ID=your_zone_id_from_cloudflare
CLOUDFLARE_RECORD_ID=your_dns_record_id
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
CLOUDFLARE_RECORD_NAME=your_domain.com

# Email for ddclient
DDCLIENT_EMAIL=info@smartmur.lab

# Your domain
DDCLIENT_DOMAIN=smartmur.lab

# Timezone
TZ=UTC
