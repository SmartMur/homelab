# Traefik Environment Variables
# Copy this file to .env and fill in your actual values

# Domain Configuration
DOMAIN=smartmur.lab

# Cloudflare Configuration (for DNS challenge)
CLOUDFLARE_EMAIL=info@smartmur.lab
CLOUDFLARE_API_KEY=your_cloudflare_global_api_key
# OR use API Token (recommended):
CLOUDFLARE_DNS_API_TOKEN=your_cloudflare_dns_api_token

# Let's Encrypt Email
LETSENCRYPT_EMAIL=info@smartmur.lab

# Dashboard Configuration
# Generate with: echo $(htpasswd -nb admin your_password) | sed -e s/\\$/\\$\\$/g
TRAEFIK_DASHBOARD_AUTH=admin:$$apr1$$CHANGE_ME_GENERATE_HTPASSWD

# Timezone
TZ=UTC

# Network settings
TRAEFIK_NETWORK=proxy
