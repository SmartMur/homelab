---
# Ansible Secrets Template
# Copy this file and encrypt with: ansible-vault encrypt secrets.yml
# Then edit with: ansible-vault edit secrets.yml

# System Credentials
ansible_become_password: "CHANGE_ME"
ansible_ssh_password: "CHANGE_ME"

# Database Passwords
mysql_root_password: "CHANGE_ME_GENERATE_SECURE_PASSWORD"
postgresql_password: "CHANGE_ME_GENERATE_SECURE_PASSWORD"

# API Keys and Tokens
cloudflare_api_token: "CHANGE_ME"
github_token: "CHANGE_ME"
docker_registry_password: "CHANGE_ME"

# Service Credentials
smtp_password: "CHANGE_ME"
backup_encryption_key: "CHANGE_ME"

# Certificates (if needed)
# ssl_certificate: |
#   -----BEGIN CERTIFICATE-----
#   CHANGE_ME
#   -----END CERTIFICATE-----
# 
# ssl_private_key: |
#   -----BEGIN PRIVATE KEY-----
#   CHANGE_ME
#   -----END PRIVATE KEY-----
