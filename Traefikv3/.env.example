# Traefik v3 Environment Variables
# Copy this file to .env and fill in your actual values

# Domain Configuration
DOMAIN=smartmur.lab

# Cloudflare API Token (for DNS challenge)
# Generate at: https://dash.cloudflare.com/profile/api-tokens
# Required permissions: Zone:DNS:Edit
CF_API_TOKEN=your_cloudflare_dns_api_token

# Let's Encrypt Email
ACME_EMAIL=info@smartmur.lab

# Dashboard Configuration
# Generate with: echo $(htpasswd -nb admin your_password) | sed -e s/\\$/\\$\\$/g
TRAEFIK_DASHBOARD_USER=admin
TRAEFIK_DASHBOARD_PASSWORD=$$apr1$$CHANGE_ME_GENERATE_HTPASSWD

# Timezone
TZ=UTC

# Network
TRAEFIK_NETWORK=proxy

# Log Level (DEBUG, INFO, WARN, ERROR)
LOG_LEVEL=INFO
